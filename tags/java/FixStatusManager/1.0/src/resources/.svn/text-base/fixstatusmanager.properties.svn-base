application_uid=FixStatusManager-demo#

##
## Websocket server settings <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<
##
websocket_connection_port=4444
websocket_host_name=localhost

# how ofter to check  sec
price_check_interval=300

##is websocket server is secured(TLS tunneling enabled)
secured_socket=false

##path to keystore
tls_key=../tls_cert/sockets_v-kurse_com.jks
tls_keystore_type=JKS

##
## Linked MySQL data base information <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<
##

jdbc_props_path=../jdbc_demo.properties

##
## Run command on Failure <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<
##

fix_failure_rstcmd=sh runonfailure
